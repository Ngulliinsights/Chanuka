{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:debug": "VITE_DEBUG=true VITE_DEBUG_WARNINGS=true VITE_DEBUG_REQUESTS=true VITE_DEBUG_FILE_CHANGES=true VITE_DEBUG_MODULE_INVALIDATION=true tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/public",
    "build:server": "echo 'Server build skipped - using development mode'",
    "start": "tsx server/index.ts",
    "db:migrate": "tsx scripts/database/migrate.ts || echo 'Migration skipped - continuing with development'",
    "db:setup": "npm run db:migrate || echo 'Database setup skipped - using fallback'",
    "dev:safe": "npm run dev",
    "db:generate": "tsx scripts/database/generate-migration.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx db/seed.ts",
    "migrate": "tsx server/scripts/migration-runner.ts",
    "migrate:up": "tsx server/scripts/migration-runner.ts up",
    "migrate:down": "tsx server/scripts/migration-runner.ts down",
    "migrate:status": "tsx server/scripts/migration-runner.ts status",
    "migrate:validate": "tsx server/scripts/migration-runner.ts validate",
    "migrate:create": "tsx server/scripts/migration-runner.ts create",
    "test": "npm run test:backend && npm run test:frontend && npm run test:e2e",
    "test:backend": "jest --config jest.backend.config.js --runInBand",
    "test:backend:watch": "jest --config jest.backend.config.js --watch --runInBand",
    "test:backend:coverage": "jest --config jest.backend.config.js --coverage --runInBand",
    "test:backend:performance": "jest --config jest.backend.config.js --testPathPattern=performance --runInBand",
    "test:backend:integration": "jest --config jest.backend.config.js --testPathPattern=integration --runInBand",
    "test:database": "jest --config jest.backend.config.js --testPathPattern=database --runInBand",
    "test:frontend": "vitest run --config vitest.frontend.config.ts",
    "test:frontend:watch": "vitest --config vitest.frontend.config.ts",
    "test:frontend:ui": "vitest --ui --config vitest.frontend.config.ts",
    "test:frontend:coverage": "vitest run --coverage --config vitest.frontend.config.ts",
    "test:frontend-serving": "node scripts/testing/run-frontend-serving-tests.js",
    "test:frontend-serving:server": "jest --config jest.backend.config.js --testPathPattern=frontend-serving --runInBand",
    "test:frontend-serving:client": "vitest run client/src/__tests__/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:validate-config": "node scripts/validate-test-config.js",
    "db:test": "tsx scripts/database/test-connection.ts",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:api": "playwright test --project=api",
    "test:performance": "playwright test tests/performance",
    "test:performance:db": "playwright test --project=database-performance",
    "test:integration": "playwright test --project=integration",
    "test:visual": "playwright test --grep @visual",
    "test:migration": "npm run test:performance:db && npm run test:integration && npm run test:api",
    "optimize:memory": "tsx scripts/optimize-memory.js",
    "cleanup:memory": "node --expose-gc scripts/immediate-memory-cleanup.cjs",
    "analyze:bundle": "webpack-bundle-analyzer dist/public/assets/*.js",
    "analyze:bundle:advanced": "node scripts/analyze-bundle.js",
    "analyze:bundle:open": "node scripts/analyze-bundle.js --open",
    "setup:playwright": "node scripts/setup-playwright.js",
    "test:strategic": "node scripts/run-strategic-tests.js",
    "audit:codebase-utilities": "tsx scripts/audit-codebase-utilities.ts",
    "migrate:codebase-utilities": "tsx scripts/migrate-codebase-utilities.ts",
    "migrate:codebase-utilities:dry-run": "tsx scripts/migrate-codebase-utilities.ts --dry-run",
    "migrate:api-utilities": "tsx scripts/migrate-codebase-utilities.ts --category=api",
    "migrate:logging-utilities": "tsx scripts/migrate-codebase-utilities.ts --category=logging",
    "migrate:performance-utilities": "tsx scripts/migrate-codebase-utilities.ts --category=performance",
    "audit:middleware-sprawl": "tsx scripts/audit-middleware-sprawl.ts",
    "audit:error-handling-sprawl": "tsx scripts/audit-error-handling-sprawl.ts",
    "migrate:middleware": "tsx scripts/migrate-middleware.ts",
    "migrate:error-handling": "tsx scripts/migrate-error-handling.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.2",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.15.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^7.7.0",
    "recharts": "^2.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.3.5",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.4",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.27.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^3.4.14",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vite": "^5.4.15",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
