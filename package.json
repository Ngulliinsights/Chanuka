    "migrate:up": "tsx server/scripts/migration-runner.ts up",
    "migrate:down": "tsx server/scripts/migration-runner.ts down",
    "migrate:status": "tsx server/scripts/migration-runner.ts status",
    "migrate:validate": "tsx server/scripts/migration-runner.ts validate",
    "migrate:create": "tsx server/scripts/migration-runner.ts create",
    "test": "nx run-many --target=test --all",
    "test:nx:client": "nx test client",
    "test:nx:server": "nx test server",
    "test:nx:shared": "nx test shared",
    "lint": "nx run-many --target=lint --all",
    "lint:nx:client": "nx lint client",
    "lint:nx:server": "nx lint server",
    "lint:nx:shared": "nx lint shared",
    "lint:css": "stylelint \"client/src/**/*.css\"",
