    "db:migrate:validate": "tsx scripts/database/migrate.ts --validate",
    "db:migrate:test": "tsx scripts/database/migrate.ts --test --performance",
    "db:migrate:dry-run": "tsx scripts/database/migrate.ts --dry-run",
