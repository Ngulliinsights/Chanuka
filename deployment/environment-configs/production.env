# Production Environment Configuration
REACT_APP_ENV=production
REACT_APP_API_BASE_URL=https://api.chanuka.ke
REACT_APP_WS_URL=wss://ws.chanuka.ke
REACT_APP_CDN_URL=https://cdn.chanuka.ke

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Monitoring
REACT_APP_SENTRY_DSN=https://your-production-sentry-dsn@sentry.io/project-id
REACT_APP_SENTRY_ENVIRONMENT=production
REACT_APP_SENTRY_SAMPLE_RATE=0.1
REACT_APP_SENTRY_TRACES_SAMPLE_RATE=0.1

# Performance
REACT_APP_PERFORMANCE_BUDGET_JS=100
REACT_APP_PERFORMANCE_BUDGET_CSS=50
REACT_APP_PERFORMANCE_BUDGET_IMAGES=300

# Security
REACT_APP_CSP_NONCE_ENABLED=true
REACT_APP_ENABLE_HTTPS_ONLY=true

# Development tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=false
REACT_APP_ENABLE_REACT_DEVTOOLS=false
REACT_APP_LOG_LEVEL=error