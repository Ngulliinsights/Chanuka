# Development Environment Configuration
REACT_APP_ENV=development
REACT_APP_API_BASE_URL=https://dev-api.chanuka.ke
REACT_APP_WS_URL=wss://dev-ws.chanuka.ke
REACT_APP_CDN_URL=https://dev-cdn.chanuka.ke

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_MODE=true

# Monitoring
REACT_APP_SENTRY_DSN=https://your-dev-sentry-dsn@sentry.io/project-id
REACT_APP_SENTRY_ENVIRONMENT=development
REACT_APP_SENTRY_SAMPLE_RATE=1.0
REACT_APP_SENTRY_TRACES_SAMPLE_RATE=1.0

# Performance
REACT_APP_PERFORMANCE_BUDGET_JS=200
REACT_APP_PERFORMANCE_BUDGET_CSS=100
REACT_APP_PERFORMANCE_BUDGET_IMAGES=500

# Security
REACT_APP_CSP_NONCE_ENABLED=false
REACT_APP_ENABLE_HTTPS_ONLY=false

# Development tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_DEVTOOLS=true
REACT_APP_LOG_LEVEL=debug