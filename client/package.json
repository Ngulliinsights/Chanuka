{
  "name": "@chanuka/client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/contrast-check.js",
    "build": "vite build",
    "build:development": "vite build --mode development --config vite.production.config.ts",
    "build:staging": "vite build --mode staging --config vite.production.config.ts",
    "build:pre-production": "vite build --mode pre-production --config vite.production.config.ts",
    "build:production": "vite build --mode production --config vite.production.config.ts",
    "build:analyze": "tsx scripts/build-analyzer.ts",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "jest --config=jest.a11y.config.js",
    "test:unit": "vitest run --coverage --reporter=verbose",
    "test:integration": "vitest run --config=vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "vitest run --config=vitest.performance.config.ts",
    "test:performance-budget": "node scripts/performance-budget-check.js",
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:comprehensive": "tsx src/__tests__/scripts/run-comprehensive-tests.ts",
    "test:all": "npm run test:comprehensive",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "audit:design-system": "node scripts/design-system-audit.js",
    "analyze:bundle": "vite build --mode analyze",
    "analyze:bundle-ci": "ANALYZE=true vite build && node scripts/check-performance-budget.js",
    "analyze:bundle:advanced": "node scripts/advanced-bundle-analyzer.js",
    "analyze:radix": "node scripts/radix-bundle-analyzer.js",
    "check:performance-budget": "node scripts/check-performance-budget.js",
    "optimize:assets": "node scripts/optimize-assets.js",
    "preview": "vite preview",
    "cleanup": "node scripts/unregister-sw.js",
    "deploy:staging": "node ../scripts/deploy-production.js --environment staging",
    "deploy:pre-production": "node ../scripts/deploy-production.js --environment pre-production",
    "deploy:production": "node ../scripts/deploy-production.js --environment production",
    "deploy:production:dry-run": "node ../scripts/deploy-production.js --environment production --dry-run",
    "deploy:integration": "tsx scripts/deployment-integration.ts",
    "monitoring:setup": "node ../deployment/monitoring-dashboards.js",
    "cdn:deploy": "node ../deployment/cdn-config.js"
  },
  "dependencies": {
    "@chanuka/shared": "workspace:*",
    "@datadog/browser-rum": "^6.23.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.10.1",
    "@sentry/browser": "^10.23.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/d3": "^7.4.3",
    "@types/react-virtualized": "^9.22.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "fuse.js": "^7.1.0",
    "immer": "^10.0.3",
