{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/index.ts",
    "src/main.tsx",
    "server/index.ts",
    "client/src/main.tsx"
  ],
  "project": [
    "**/*.{js,jsx,ts,tsx}"
  ],
  "ignore": [
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/dist/**",
    "**/build/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "@types/*"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false
}
