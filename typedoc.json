{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "client/src/infrastructure/api/index.ts",
    "client/src/infrastructure/asset-loading/index.ts",
    "client/src/infrastructure/auth/index.ts",
    "client/src/infrastructure/browser/index.ts",
    "client/src/infrastructure/cache/index.ts",
    "client/src/infrastructure/command-palette/index.ts",
    "client/src/infrastructure/community/index.ts",
    "client/src/infrastructure/consolidation/index.ts",
    "client/src/infrastructure/error/index.ts",
    "client/src/infrastructure/events/index.ts",
    "client/src/infrastructure/hooks/index.ts",
    "client/src/infrastructure/logging/index.ts",
    "client/src/infrastructure/mobile/index.ts",
    "client/src/infrastructure/navigation/index.ts",
    "client/src/infrastructure/observability/index.ts",
    "client/src/infrastructure/personalization/index.ts",
    "client/src/infrastructure/recovery/index.ts",
    "client/src/infrastructure/search/index.ts",
    "client/src/infrastructure/security/index.ts",
    "client/src/infrastructure/storage/index.ts",
    "client/src/infrastructure/store/index.ts",
    "client/src/infrastructure/sync/index.ts",
    "client/src/infrastructure/system/index.ts",
    "client/src/infrastructure/validation/index.ts",
    "client/src/infrastructure/workers/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "name": "Chanuka Infrastructure API Documentation",
  "includeVersion": true,
  "readme": "client/src/infrastructure/README.md",
  "tsconfig": "client/tsconfig.json",
  "exclude": [
    "**/*+(.test|.spec).ts",
    "**/__tests__/**/*",
    "**/node_modules/**/*"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "API",
    "State Management",
    "Error Handling",
    "Observability",
    "Authentication",
    "Validation",
    "Navigation",
    "Storage",
    "Security",
    "Mobile",
    "Browser",
    "Workers",
    "Events",
    "Hooks",
    "Utilities",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "Variable",
    "Enum"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "plugin": [],
  "theme": "default",
  "lightHighlightTheme": "light-plus",
  "darkHighlightTheme": "dark-plus",
  "customCss": "",
  "basePath": ".",
  "githubPages": false,
  "hideGenerator": false,
  "searchInComments": true,
  "cleanOutputDir": true,
  "titleLink": "/",
  "navigationLinks": {
    "GitHub": "https://github.com/your-org/chanuka-platform"
  },
  "sidebarLinks": {},
  "commentStyle": "jsdoc",
  "blockTags": [
    "@deprecated",
    "@param",
    "@returns",
    "@throws",
    "@example",
    "@see",
    "@link",
    "@module",
    "@category"
  ],
  "inlineTags": [
    "@link",
    "@linkcode",
    "@linkplain"
  ],
  "modifierTags": [
    "@public",
    "@private",
    "@protected",
    "@internal",
    "@readonly",
    "@packageDocumentation"
  ]
}
