# Development Environment - Aggressive Error Suppression
# These settings maximize console cleanliness in development

# Completely disable problematic features
VITE_ENABLE_CSP=false
VITE_ENABLE_CSRF=false
VITE_ENABLE_VULNERABILITY_SCANNING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_SECURITY_MONITORING=false
VITE_ENABLE_ACTIVITY_TRACKING=false

# HMR Configuration
VITE_HMR_PORT=5174
VITE_HMR_OVERLAY=false
VITE_HMR_CLIENT_PORT=5174

# Logging Configuration
VITE_LOG_LEVEL=error
VITE_SUPPRESS_DEV_ERRORS=true
VITE_SUPPRESS_WARNINGS=true

# Session Configuration
VITE_SESSION_CHECK_INTERVAL=600000
VITE_DISABLE_ACTIVITY_TRACKING=true
VITE_DISABLE_SESSION_MONITORING=true

# Performance Configuration
VITE_DISABLE_REALTIME_OPTIMIZATION=true
VITE_DISABLE_WEB_VITALS=true
VITE_DISABLE_BUNDLE_ANALYSIS=true
VITE_DISABLE_ASSET_OPTIMIZATION=true

# Backend Configuration
VITE_API_BASE_URL=http://localhost:5000
VITE_MOCK_API=true
VITE_SKIP_BACKEND_CALLS=true

# React Configuration
VITE_REACT_STRICT_MODE=false
VITE_REACT_DEVTOOLS=false

# Additional suppressions for remaining messages
VITE_SUPPRESS_HMR_WARNINGS=true
VITE_SUPPRESS_WEBSOCKET_ERRORS=true
VITE_SUPPRESS_BACKEND_ERRORS=true
VITE_SUPPRESS_SERVICE_WORKER_LOGS=true
VITE_SUPPRESS_VITE_WARNINGS=true
VITE_SUPPRESS_EXTENSION_ERRORS=true
